import java.io.*;
import java.util.*;

public class MainWindow {
    public static void main(String[] args) throws IOException {
        Scanner scan = new Scanner(new File("./src/ListOfPlayers"));

        BufferedReader reader = new BufferedReader(new FileReader("./src/ListOfPlayers"));
        int lines = 0;
        while (reader.readLine() != null) lines++;

        for (int i = 0; i < lines; i++) {
            Player deux = assigner(scan);
            String e = deux.toString();
            System.out.print(e);
            System.out.println();
        }

//        Player ki;
//        ki = Inputter.assigner(scan);
//        String e = ki.toString();
//        System.out.print(ki);
//
//
//
    }

        public static Player assigner(Scanner sc) throws FileNotFoundException {
            String name, college, group, block, split[];
            int age;
            double height, weight;


            block = sc.nextLine();
            split = block.split("\\/");
            name = split[0];
            group = split[1];
            college = split[2];
            age = Integer.parseInt(split[3]);
            weight = Double.parseDouble(split[4]);
            height = Double.parseDouble(split[5]);


            return new Player(name, group, college, age, weight, height);


        }
    }
