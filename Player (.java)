import java.io.FileNotFoundException;
import java.util.*;

public class Player {
    private String name, team, college;
    private int  age;
    private double weight, height;

    public Player(String nick, String group, String uni, int old, double mass, double tall) {
        name = nick;
        team = group;
        college = uni;
        age = old;
        weight = mass;
        height = tall;
    }


    public String toString() {
        String description = "";
        description += "Name: " + name + "\n";
        description += "Age: " + age + "\n";
        description += "Team: " + team + "\n";
        description += "College: " + college + "\n";
        description += "Weight " + weight + "\n";
        description += " Height: " + height + "\n";

        return description;
    }


}
